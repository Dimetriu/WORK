<% @todo_lists.each do |todo_list| %>
<div class="links">
  <% if user_signed_in? %>
    <p>Hello!, <%= current_user.email %></p>
    <%= link_to 'Sign out', destroy_user_session_path, method: :delete %>
  <% else %>
    <%= link_to 'Sign up', new_user_session_path %> 
    <%= link_to 'Sign in', new_user_registration_path %>    
  <% end %> 
</div>

  <div class="index_row clearfix">
    <h2 class="todo_list_title"><%= link_to todo_list.title, todo_list %></h2>
    <p class="todo_list_sub_title"><%= todo_list.description %></p>
  </div>  
<% end %>

<div class="links">
  <%= link_to "ADD TODO", new_todo_list_path %>
</div>